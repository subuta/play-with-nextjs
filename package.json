    "babel-preset-stage-0": "^6.24.1",
    "isomorphic-unfetch": "^2.0.0",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "next": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-free-style": "^7.0.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2"
  },
  "devDependencies": {
