    "watch": "backpack dev",
    "test": "ava",
    "serve": "NODE_ENV=production node dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/play-with-nextjs.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/play-with-nextjs/issues"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
